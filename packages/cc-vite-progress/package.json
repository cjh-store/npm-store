{
  "name": "cc-vite-progress",
  "version": "1.0.0",
  "description": "A progress bar plugin for Vite.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "yarn run build --watch --ignore-watch examples",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeddygong/vite-plugin-progress.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "progress"
  ],
  "author": "jeddygong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeddygong/vite-plugin-progress/issues"
  },
  "homepage": "https://github.com/jeddygong/vite-plugin-progress#readme",
  "peerDependencies": {
    "vite": ">2.0.0-0"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/progress": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "bumpp": "^7.2.0",
    "eslint": "^8.50.0",
    "esno": "^0.16.3",
    "tsup": "^5.12.9",
    "typescript": "^4.9.5",
    "vite": "*"
  },
  "dependencies": {
    "picocolors": "^1.0.0",
    "progress": "^2.0.3",
    "rd": "^2.0.1"
  }
}
